.cointainer {
  position: fixed;
  height: 100%;
  width: 100%;
}

.search {
  width: 100%;
  background: #fff;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.search-box {
  border-radius: 60rpx 0px 0px 60rpx;
  height: 60rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  padding-left: 70rpx;
  background-size: 40rpx;
  background-position: 16rpx center;
  background-color: #f5f7fa;
}

.chat-main {
  height: 100%;
}

.shadow {
  background-color: #c1c1c1;
}

.search-box-right {
  background-color: #f5f7fa;
  border-radius: 0px 60rpx 60rpx 0px;
  height: 60rpx;
  width: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.focusSearch {
  height: 100rpx;
}

.focusSearchBox {
  height: 80rpx;
}

.search-main {
  background-color: #f5f7fa;
  height: 100%;
}

.cancelsearch {
  width: 80rpx;
  height: 50rpx;
  font-size: 10px;
  line-height: 50rpx;
  border: 1px #f5f7fa solid;
  background: #fff;
  padding-left: 2rpx;
  padding-right: 2rpx;
  color: #8a8a8a;
}

.chatListBox {
  position: relative;
  border: 1px #e3e5eb solid;
  margin: 20rpx 30rpx;
  border-radius: 24rpx;
  padding: 12rpx 24rpx 24rpx 24rpx;
  background-color: #fff;
  box-shadow: 1px 1px 8px #e3e5eb;
}

image.head {
  border-radius: 50%;
  width: 70rpx;
  height: 70rpx;
}

.chatListBox .cusname {
  font-size: 28rpx;
  font-weight: 600;
  padding: 0rpx 30rpx;
  color: #3b3f4b;
}

.chatListBox .cusinfo {
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  align-items: center;
}

.arrow {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  left: 296rpx;
}

.quick-chat-box {
  margin-left: 100rpx;
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.quick-chat-box .msg {
  font-size: 12px;
  color: #3b3f4b;
}

.quick-chat-box .time {
  font-size: 8px;
  color: #dee1e9;
  font-weight: bolder;
}

.quick-chat-box view.reply {
  display: inline-block;
  border: 1rpx #eef0f4 solid;
  border-radius: 50%;
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
}

.quick-chat-box .right {
  width: 112rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.quick-chat-box .reply image {
  width: 26rpx;
  height: 26rpx;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: #999;
  z-index: 9999;
  top: 100rpx;
  left: 0;
  opacity: 0.5;
}

.flag {
  width: 48rpx;
  height: 48rpx;
  top: -8rpx;
  left: 610rpx;
  position: absolute;
}

.flag image {
  width: 48rpx;
  height: 48rpx;
}

.flag text {
  position: absolute;
  color: #fff;
  font-size: 20rpx;
  width: 100%;
  text-align: center;
  top: 6rpx;
  font-weight: bold;
}

.openlist {
  border-bottom: 0.5px #f0f1f2 solid;
  height: 76rpx;
}

.openlist:last-child {
  border-bottom: none;
}

.reply-box {
  background: #fafbfc;
  width: 100%;
  position: fixed;
  bottom: 0px;
}

.reply-box .sumary {
  display: block;
  padding: 10rpx 20rpx 10rpx 20rpx;
  border: 0.5px #e5e8eb solid;
  border-left: none;
  border-right: none;
  line-height: 44rpx;
}

.reply-box .sumary image {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 68rpx;
  top: 26rpx;
}

.reply-box text {
  font-size: 24rpx;
  color: #969697;
  font-weight: bold;
}

.reply-input-cointainer {
  background: #fefefe;
  height: 80rpx;
  display: flex;
  align-items: center;
}

.reply-input-cointainer input {
  border: 0.5px #e5e8eb solid;
  width: 560rpx;
  margin-left: 20rpx;
  height: 60rpx;
  border-radius: 60rpx;
  padding-left: 20rpx;
}

.reply-input-cointainer image {
  border: 0.5px #e5e8eb solid;
  border-radius: 50%;
  padding: 16rpx;
  width: 30rpx;
  height: 30rpx;
  margin-left: 35rpx;
}
