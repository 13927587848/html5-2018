@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-my:before { content: "\e608"; }

.icon-lianxi1:before { content: "\e61a"; }

.icon-jiaoyu:before { content: "\e624"; }
/*globar set*/
.cls
{
  clear: both;
}
page
{
  background: #eee;
  padding-bottom: 130rpx;
}
#head
{
  background: white;
  height: 400rpx;
  display: flex;
  flex-flow: column;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  border-bottom:1rpx solid #ccc;
}
#head-con
{
  width: 80%;
  margin: auto;
  padding: 10% 0;
}
/*name*/
#head #head-con open-data:nth-child(1)
{
  font-size: 34rpx;
  line-height: 2;
}
#head #head-con open-data:nth-child(2)
{
  float: right;
  width: 150rpx;
  height: 150rpx;
  display: block;
  border-radius: 50%;
  overflow: hidden;
}
#head #head-con .head-con-desc
{
  display: block;
  font-size: 24rpx;
  color: #666;
}
/*info*/
#main
{
  font-size: 28rpx;
}
#main-info
{
  background: white;
  margin-top: 30rpx;
  border-radius: 20rpx;
  overflow: hidden;
  padding-bottom: 15rpx;
  border-bottom:1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
}
view.main-info-summary
{
  background-image: linear-gradient(-45deg, #FFC796 0%, #FF6B95 100%);
  padding-left: 75rpx;
  line-height: 4;
  color: white;
}
#main-info .main-info-con
{
  width: 80%;
  padding: 10rpx;
  margin: 20rpx auto;
}
#main-info text.main-info-title
{
  color: black;
  position: relative;
}
#main-info text.main-info-title::after
{
  content: "|";
  position: absolute;
  top: 0;
  right: -20rpx;
  bottom: 0;
  color: #555;
}
#main text.main-info-desc
{
  color: #555;
  font-weight: normal;
  margin-left: 30rpx;
}
/*occupation search*/
#main-occupation
{
  margin-top: 30rpx;
  overflow: hidden;
  border-radius: 20rpx;
  background: white;
  padding-bottom: 30rpx;
  border-bottom:1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
}
#main-occupation view.main-info-summary
{
  background-image: linear-gradient(to left, #48c6ef 0%, #5061fd 100%);
  margin-bottom: 30rpx;
}
#main-occupation .main-occ-item
{
  width: 80%;
  margin: auto;
  height: 100rpx;
}
#main-occupation .main-occ-item view.main-occ-item-img
{
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  float: left;
}
#main-occupation .main-occ-item image
{
  width: 100%;
  height: 100%;
  position: relative;
}
.main-occ-item-desc
{
  
  margin-left: 130rpx;
  height: 100rpx;
  line-height: 1.8;
}
#main-occupation .main-occ-item .main-occ-name
{

}
#main-occupation .main-occ-item .main-occ-salary
{
  color: #999;
  display: block;
}
/*footer*/
.footer {
  position: fixed; 
  bottom: 0;
  width: 100%;
  height:100rpx; /*footer的高度*/
  background: rgba(255,255,255,.95);
  z-index: 500;
  border-top:1rpx solid #ccc;
  display: flex;
  flex-direction: row;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.footer_list{
width:17%;
height:100%;
text-align:center;
padding-top:8rpx;
margin-left:60rpx;
margin-right:62rpx;
}
.footer-image{
   width:40%;
   height:45%;
}
.footer-icon
{
  color: #999;
  font-size: 40rpx;
  margin-top: 8rpx;
}
.footer-text{
    font-size: 24rpx;
    
}
.footer-text,
.footer-icon
{
  color: #999;
  position: relative;
}
.footer-select
{
  color: #FF6B95;
}
.footer-select:first-child::after
{
  content:'';
  position: absolute;
  top: -15rpx;
  bottom: 55rpx;
  left: 25rpx;
  right: 25rpx; 
  background:linear-gradient(-45deg, #FFC796 0%, #FF6B95 100%);
  border-radius: 20rpx;
}
