/*font set
--------------------------*/
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-daifukuan:before { content: "\f013c"; }
.icon-set:before { content: "\e6ae"; }
.icon-daishouhuo:before { content: "\e622"; }
.icon-daipingjia:before { content: "\e608"; }
.icon-icon-test:before { content: "\e615"; }
/*全局设置
--------------------------*/
page
{
  background: #eee;
}
#head,#head #head-con open-data:nth-child(1)
{
  width: 100%;
}
#head #set,#head #head-con open-data:last-child
{
  color: white;
}
#main-head,#main-con,#main-about
{
  background:white;
}
#main-about view,
#main-con-head
{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  padding-left: 45rpx;
  padding-right: 45rpx;
}
#main-about text,
#main-con-head text
{
  font-size: 20rpx;
  color: #555;
  float: right;
  line-height: 100rpx;
}
#main-about,
#main-con
{
  border-radius: 20rpx;
  
}
/*head block
--------------------------*/
#head
{
  background-image: linear-gradient(-45deg, #FFC796 0%, #FF6B95 100%);
  height: 450rpx;
  display: flex;
}
#head #set
{
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  font-size: 130%;
}
#head #head-con
{
  width: 20%;
  margin: auto;
}
#head #head-con open-data:nth-child(1)
{
  height: calc(750rpx * 0.2);
  display: block;
  border-radius: 50%;
  overflow: hidden;
  border: 6rpx solid white;
}
#head #head-con open-data:last-child
{
  text-align: center;
  display: block;
  line-height: 3;
  font-size: 28rpx;
}
/*收藏店铺组件*/
#main-head
{
  height: 100rpx;
  border-bottom-right-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
}
#main-head .main-head-li
{
  float: left;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #555;
  width: 200rpx;
  margin: 0 25rpx;
  text-align: center;
}
/*全部订单组件*/
#main-con
{
  margin-top: 20rpx;
}
#main-con-ul
{
  clear: both;
  height: 170rpx;
}
#main-con-ul .main-con-ul-li
{
  color:#FF6B95;
  float: left;
  font-size: 200%;
  width: 150rpx;
  margin: 35rpx 18rpx 25rpx;
  text-align: center;
}
#main-con-ul .main-con-ul-li view
{
  font-size: 20rpx;
  color: #555;
  line-height: 1.8;
}
#main-con-head
{
  border-bottom: 1rpx solid #ddd;
}
/*关于作者*/
#main-about
{
  margin-top: 20rpx;
}

