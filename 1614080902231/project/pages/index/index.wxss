.weui_cell {
    position: relative;
    display: block;
    padding: 15px;
    border-bottom: 1px solid #dadada;
}
.weui_cell image {
  width: 100%;
  height: 150px;
  background: #ebebeb;
  display: block;
  margin: 0 auto;
}
.weui_cell_hd{
    height: 20px;
    width: 100%;
}
.weui_cell_hd p{
    display: block;
    font-size: 12px;
    color: #939393;
    text-align: center;
}
